{
  "name": "peer-evaluation-lsbu",
  "description": "The full stack web app of the Peer Evaluation App of LSBU",
  "version": "1.0.0-staging.13",
  "private": true,
  "author": "Juan Carlos Blanco Delgado <juancarlosjr97@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "dev:docker": "docker-compose up",
    "dev:next": "next dev",
    "dev:tsc": "yarn tsc --watch --noEmit --incremental --preserveWatchOutput --pretty",
    "dev": "concurrently --kill-others npm:dev:*",
    "build:next": "next build",
    "start": "next start",
    "build:start": "next build && next start &",
    "build:start:background": "yarn build:start &",
    "lint": "next lint --dir . --file ./.eslintrc.json",
    "lint:check": "yarn lint && yarn tsc",
    "lint:fix": "next lint --fix --dir . --file ./.eslintrc.json",
    "prettier": "prettier --write .",
    "lint:fix:all": "yarn prettier && yarn lint:fix",
    "semantic-release": "semantic-release",
    "docker:ci": "yarn dev:docker -d",
    "clear:cache": "concurrently \"rm -rf .next\" \"rm -rf .nyc_output\" \"rm -rf coverage\" \"rm -rf node_modules\" \"rm -rf tsconfig.tsbuildinfo\" \"rm -rf .eslintcache\"",
    "test": "concurrently --kill-others --max-processes 1 npm:test:jest:ci npm:test:cypress:ci npm:test:cypress:ct:ci",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:ci": "jest --ci",
    "test:jest:clear-cache": "jest --clearCache",
    "test:cypress:ci": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:ct:ci": "cypress run-ct",
    "test:cypress:ct:open": "cypress open-ct",
    "test:coverage:merge": "istanbul-merge --out coverage/merged/coverage-final.json coverage/jest/coverage-final.json ./coverage/cypress/coverage-final.json",
    "test:coverage:report": "nyc report --reporter=lcov --reporter=text --reporter=text-summary --temp-dir=./coverage/merged --report-dir=./coverage/merged",
    "test:coverage:check": "yarn test:coverage:merge && yarn test:coverage:report --check-coverage --lines 50",
    "test:unit": "concurrently --kill-others --max-processes 1 npm:test:jest:ci npm:test:cypress:ct",
    "prisma:seed": "npx prisma db seed",
    "prisma:seed:prod": "npx ts-node --project ./tsconfig.prisma.json prisma/seed.production.ts",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "postinstall": "yarn prisma:generate",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:migrate:create": "npx prisma migrate dev",
    "prisma:migrate:reset:create": "npx prisma migrate reset",
    "prisma:db:push": "npx prisma db push",
    "prisma:reset:prod": "npx prisma migrate reset --skip-seed --force --skip-generate",
    "prisma:format": "npx prisma format",
    "prepare": "husky install"
  },
  "nyc": {
    "report-dir": "./coverage/cypress"
  },
  "prisma": {
    "seed": "ts-node --project ./tsconfig.prisma.json prisma/seed.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@date-io/date-fns": "^2.13.1",
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@fontsource/roboto": "4.5.2",
    "@mui/icons-material": "^5.4.2",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "5.3.1",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "^3.9.2",
    "@sentry/nextjs": "6.17.3",
    "@types/date-fns": "^2.6.0",
    "apollo-server-micro": "3.6.2",
    "class-validator": "0.13.2",
    "formik": "^2.2.9",
    "graphql": "15.3.0",
    "graphql-fields": "2.0.3",
    "graphql-middleware": "6.0.9",
    "graphql-shield": "7.5.0",
    "liquidjs": "^9.34.0",
    "lodash": "^4.17.21",
    "micro": "9.3.4",
    "mui-datatables": "^4.1.2",
    "next": "12.0.9",
    "next-auth": "^4.3.4",
    "nodemailer": "6.7.2",
    "papaparse": "^5.3.1",
    "pino": "^7.6.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.2.0",
    "react-quill": "^1.3.5",
    "react-spinners": "^0.11.0",
    "react-tiny-fab": "^4.0.4",
    "react-use": "^17.4.0",
    "react-window": "^1.8.6",
    "reflect-metadata": "0.1.13",
    "swr": "^1.2.2",
    "type-graphql": "1.1.1",
    "typegraphql-prisma": "0.19.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/react": "^5.12.3",
    "@cypress/webpack-dev-server": "^1.8.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "13.5.0",
    "@types/mui-datatables": "^3.7.8",
    "@types/node": "17.0.11",
    "@types/nodemailer": "^6.4.4",
    "@types/papaparse": "^5.3.2",
    "@types/react": "17.0.38",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "5.0.4",
    "html-webpack-plugin": "^5.5.0",
    "husky": "7.0.4",
    "istanbul-merge": "^1.1.1",
    "jest": "27.4.7",
    "lint-staged": "12.3.2",
    "local-cypress": "^1.2.3",
    "prettier": "2.5.1",
    "prisma": "^3.9.2",
    "semantic-release": "19.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "webpack": "^5.69.1",
    "webpack-dev-server": "^4.7.4",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,json}": "prettier --write"
  },
  "engines": {
    "node": "16.13.2"
  },
  "publishConfig": {
    "access": "restricted"
  }
}

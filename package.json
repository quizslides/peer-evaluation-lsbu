{
  "name": "peer-evaluation-lsbu",
  "description": "The full stack app of the Peer Evaluation App of LSBU",
  "version": "0.0.0",
  "private": true,
  "author": "Juan Carlos Blanco Delgado <juancarlosjr97@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "semantic-release": "semantic-release",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "test:clear-cache": "jest --clearCache",
    "cypress": "cypress run",
    "prisma:studio": " npx prisma studio",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "@next-auth/prisma-adapter": "^1.0.1",
    "@prisma/client": "^3.8.1",
    "@sentry/nextjs": "^6.16.1",
    "apollo-server-micro": "^3.6.1",
    "class-validator": "^0.13.2",
    "graphql": "15.3.0",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "6.1.13",
    "graphql-shield": "^7.5.0",
    "micro": "^9.3.4",
    "next": "12.0.7",
    "next-auth": "^4.1.1",
    "nodemailer": "^6.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/node": "^17.0.8",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "cypress": "^9.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.0.3",
    "husky": "^7.0.0",
    "jest": "27.4.5",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "prisma": "^3.8.1",
    "semantic-release": "^18.0.1",
    "ts-node": "^10.4.0",
    "typegraphql-prisma": "^0.18.4",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx, css,md}": "prettier --write"
  }
}

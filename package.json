{
  "name": "peer-evaluation-lsbu",
  "description": "The full stack web app of the Peer Evaluation App of LSBU",
  "version": "1.6.3",
  "private": true,
  "author": "Juan Carlos Blanco Delgado <juancarlosjr97@gmail.com>",
  "license": "UNLICENSED",
  "scripts": {
    "build:next": "next build",
    "build:start": "next build && next start &",
    "build:start:background": "yarn build:start &",
    "clear:cache": "concurrently \"rm -rf .next\" \"rm -rf .nyc_output\" \"rm -rf coverage\" \"rm -rf node_modules\" \"rm -rf tsconfig.tsbuildinfo\" \"rm -rf .eslintcache\"",
    "dev": "concurrently --kill-others npm:dev:*",
    "dev:docker": "docker-compose up",
    "dev:next": "next dev",
    "dev:tsc": "yarn tsc --watch --noEmit --incremental --preserveWatchOutput --pretty",
    "docker:ci": "yarn dev:docker -d",
    "lint": "next lint --dir . --file ./.eslintrc.json",
    "lint:check": "yarn lint && yarn tsc",
    "lint:fix": "next lint --fix --dir . --file ./.eslintrc.json",
    "lint:fix:all": "yarn prettier && yarn lint:fix",
    "postinstall": "yarn prisma:generate",
    "prepare": "husky install",
    "prettier": "prettier --write .",
    "prisma:db:push": "npx prisma db push",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:format": "npx prisma format",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:create": "npx prisma migrate dev",
    "prisma:migrate:reset:create": "npx prisma migrate reset",
    "prisma:reset:prod": "npx prisma migrate reset --skip-seed --force --skip-generate",
    "prisma:seed": "npx prisma db seed",
    "prisma:seed:prod": "npx ts-node --project ./tsconfig.prisma.json prisma/seed.production.ts",
    "prisma:studio": "npx prisma studio",
    "semantic-release": "semantic-release",
    "start": "next start",
    "test": "concurrently --kill-others --max-processes 1 npm:test:jest:ci npm:test:cypress:e2e:ci npm:test:cypress:component:ci",
    "test:coverage:check": "yarn test:coverage:merge && yarn test:coverage:report --check-coverage --lines 50",
    "test:coverage:merge": "istanbul-merge --out coverage/merged/coverage-final.json coverage/jest/coverage-final.json ./coverage/cypress/coverage-final.json",
    "test:coverage:report": "nyc report --reporter=lcov --reporter=text --reporter=text-summary --temp-dir=./coverage/merged --report-dir=./coverage/merged",
    "test:cypress:component:ci": "cypress run --component --browser chrome",
    "test:cypress:component:open": "cypress open --component --browser chrome",
    "test:cypress:e2e:ci": "cypress run --browser chrome",
    "test:cypress:e2e:open": "cypress open --e2e --browser chrome",
    "test:jest": "jest",
    "test:jest:ci": "jest --ci",
    "test:jest:clear-cache": "jest --clearCache",
    "test:jest:watch": "jest --watch",
    "test:unit": "concurrently --kill-others --max-processes 1 npm:test:jest:ci npm:test:cypress:ct"
  },
  "nyc": {
    "report-dir": "./coverage/cypress"
  },
  "prisma": {
    "seed": "ts-node --project ./tsconfig.prisma.json prisma/seed.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@auth/prisma-adapter": "^1.0.1",
    "@date-io/date-fns": "^3.0.0",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@fontsource/roboto": "5.0.12",
    "@mui/icons-material": "^5.4.2",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "5.15.15",
    "@prisma/client": "5",
    "@sentry/nextjs": "7.110.0",
    "@types/date-fns": "^2.6.0",
    "apollo-server-micro": "3.13.0",
    "class-validator": "0.14.1",
    "formik": "^2.2.9",
    "graphql": "16.8.1",
    "graphql-fields": "2.0.3",
    "graphql-middleware": "6.1.35",
    "graphql-scalars": "^1.17.0",
    "graphql-shield": "7.6.5",
    "liquidjs": "^10.0.0",
    "lodash": "^4.17.21",
    "micro": "10.0.1",
    "mui-datatables": "^4.1.2",
    "next": "14.2.1",
    "next-auth": "^4.23.1",
    "next-safe": "^3.3.0",
    "nodemailer": "6.9.13",
    "papaparse": "^5.3.1",
    "pino": "^8.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.2.0",
    "react-html-parser": "^2.0.2",
    "react-quill": "^2.0.0",
    "react-spinners": "^0.13.0",
    "react-tiny-fab": "^4.0.4",
    "react-use": "^17.4.0",
    "react-verification-input": "^4.0.0",
    "react-window": "^1.8.6",
    "reflect-metadata": "0.2.2",
    "sort-nested-json": "^0.4.0",
    "swr": "^2.0.0",
    "type-graphql": "1.1.1",
    "typegraphql-prisma": "0.27.2",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@cypress/react": "^8.0.0",
    "@cypress/webpack-dev-server": "^3.1.2",
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "10.0.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "14.5.2",
    "@types/mui-datatables": "^4.0.0",
    "@types/node": "20.12.7",
    "@types/nodemailer": "^6.4.4",
    "@types/papaparse": "^5.3.2",
    "@types/react": "18.2.78",
    "@types/react-html-parser": "^2.0.2",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "@typescript-eslint/parser": "7.6.0",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "concurrently": "^8.0.0",
    "cy-verify-downloads": "^0.2.0",
    "cypress": "^13.0.0",
    "cypress-mailhog": "^2.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "9.0.0",
    "eslint-config-next": "14.2.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-testing-library": "6.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "9.0.11",
    "istanbul-merge": "^2.0.0",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "local-cypress": "^1.2.6",
    "nyc": "^15.1.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "3.2.5",
    "prisma": "5",
    "semantic-release": "23.0.8",
    "ts-node": "10.9.2",
    "typescript": "^5.1.6",
    "webpack": "^5.69.1",
    "webpack-dev-server": "^5.0.0",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,json}": "prettier --write"
  },
  "engines": {
    "node": "20"
  },
  "publishConfig": {
    "access": "restricted"
  }
}

{
  "name": "peer-evaluation-lsbu",
  "description": "The full stack app of the Peer Evaluation App of LSBU",
  "version": "0.0.0",
  "private": true,
  "author": "Juan Carlos Blanco Delgado <juancarlosjr97@gmail.com>",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir . --file ./.eslintrc.json",
    "lint:fix": "next lint --fix --dir . --file ./.eslintrc.json",
    "semantic-release": "semantic-release",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "test:clear-cache": "jest --clearCache",
    "cypress": "cypress run",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "postinstall": "yarn prisma:generate",
    "prisma:start": "npx prisma migrate dev",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@fontsource/roboto": "4.5.2",
    "@mui/icons-material": "5.3.1",
    "@mui/material": "5.3.1",
    "@next-auth/prisma-adapter": "1.0.1",
    "@prisma/client": "3.8.1",
    "@sentry/nextjs": "6.17.3",
    "apollo-server-micro": "3.6.2",
    "class-validator": "0.13.2",
    "graphql": "15.3.0",
    "graphql-fields": "2.0.3",
    "graphql-middleware": "6.0.9",
    "graphql-shield": "7.5.0",
    "micro": "9.3.4",
    "next": "12.0.9",
    "next-auth": "4.1.2",
    "nodemailer": "6.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "0.1.13",
    "type-graphql": "1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/node": "17.0.11",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "cypress": "9.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-testing-library": "5.0.4",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1",
    "prisma": "3.8.1",
    "semantic-release": "19.0.2",
    "ts-node": "10.4.0",
    "typegraphql-prisma": "0.18.4",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,json}": "prettier --write"
  }
}
